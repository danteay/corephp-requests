{
    "name": "corephp/requests",
    "description": "A PSR-7 implementation for a simple request library",
    "keywords": [
        "psr", "psr-7", "corephp", "requests"
    ],
    "homepage": "https://github.com/danteay/corephp-requests",
    "authors": [
        {
            "name": "Eduardo Aguilar",
            "email": "dante.aguilar41@gmail.com"
        }
    ],
    "require": {
        "php": ">= 7.1",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "CorePHP\\Requests\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CorePHP\\Requests\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
